{
  "name": "@ffsm/compositor",
  "version": "0.0.2",
  "description": "A collection of declarative React utility components for simplified component composition, conditional rendering, and prop management - making React UI development more maintainable and expressive.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "author": "S3Tech <s3tech.vn@gmail.com>",
  "homepage": "https://github.com/ffsmio/monorepo/tree/main/packages/ffsmio-compositor",
  "repository": {
    "type": "git",
    "url": "https://github.com/ffsmio/monorepo/tree/main/packages/ffsmio-compositor"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./as-array": {
      "types": "./dist/as-array.d.ts",
      "default": "./dist/as-array.js"
    },
    "./as-instance": {
      "types": "./dist/as-instance.d.ts",
      "default": "./dist/as-instance.js"
    },
    "./as-node": {
      "types": "./dist/as-node.d.ts",
      "default": "./dist/as-node.js"
    },
    "./as-slot": {
      "types": "./dist/as-slot.d.ts",
      "default": "./dist/as-slot.js"
    },
    "./condition": {
      "types": "./dist/condition.d.ts",
      "default": "./dist/condition.js"
    },
    "./empty": {
      "types": "./dist/empty.d.ts",
      "default": "./dist/empty.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ],
      "as-array": [
        "./dist/as-array.d.ts"
      ],
      "as-instance": [
        "./dist/as-instance.d.ts"
      ],
      "as-node": [
        "./dist/as-node.d.ts"
      ],
      "as-slot": [
        "./dist/as-slot.d.ts"
      ],
      "condition": [
        "./dist/condition.d.ts"
      ],
      "empty": [
        "./dist/empty.d.ts"
      ]
    }
  },
  "keywords": [
    "React",
    "component",
    "composition",
    "conditional-rendering",
    "slots",
    "declarative",
    "prop-injection",
    "empty-state",
    "component-library",
    "jsx",
    "typescript",
    "utility",
    "array-manipulation",
    "element-cloning",
    "fallback-handling",
    "react-helpers",
    "ui-patterns",
    "component-slots",
    "prop-forwarding",
    "render-conditions"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "react": "19.0.0",
    "rimraf": "6.0.1",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "prepublishOnly": "yarn build"
  }
}